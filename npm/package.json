{
  "name": "@loopback/example-todo-jwt",
  "description": "A modified Todo tutorial how to build an application with JWT authentication and LoopBack 4",
  "version": "4.0.9",
  "keywords": [
    "loopback",
    "LoopBack",
    "example",
    "tutorial",
    "CRUD",
    "models",
    "todo",
    "jwt",
    "authentication"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "IBM Corp. and LoopBack contributors",
  "copyright.owner": "IBM Corp. and LoopBack contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/loopbackio/loopback-next.git",
    "directory": "examples/todo-jwt"
  },
  "engines": {
    "node": "14 || 16 || 18 || 19"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean *example-todo-jwt*.tgz dist *.tsbuildinfo package",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run rebuild",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "verify": "npm pack && tar xf loopback-todo*.tgz && tree package && npm run clean",
    "premigrate": "npm run build ",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "npm run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "rebuild": "npm run clean && npm run build",
    "prestart": "npm run rebuild",
    "start": "node ."
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@loopback/authentication": "^9.0.9",
    "@loopback/authentication-jwt": "^0.12.9",
    "@loopback/boot": "^5.0.9",
    "@loopback/core": "^4.0.9",
    "@loopback/repository": "^5.1.4",
    "@loopback/rest": "^12.0.9",
    "@loopback/rest-explorer": "^5.0.9",
    "@loopback/security": "^0.8.9",
    "@loopback/service-proxy": "^5.0.9",
    "@types/bcryptjs": "^2.4.2",
    "@types/multer": "^1.4.7",
    "bcryptjs": "^2.4.3",
    "data-uri-to-buffer": "^4.0.1",
    "geolib": "^3.3.3",
    "lb4": "^1.0.0",
    "loopback-connector-postgresql": "^6.0.2",
    "loopback-connector-rest": "^4.0.2",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.0",
    "tslib": "^2.5.0",
    "uuid": "^9.0.0",
    "validator": "^13.9.0"
  },
  "devDependencies": {
    "@loopback/build": "^9.0.9",
    "@loopback/eslint-config": "^13.0.9",
    "@loopback/http-caching-proxy": "^4.0.9",
    "@loopback/testlab": "^5.0.9",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.0.0",
    "@types/uuid": "^9.0.1",
    "@types/validator": "^13.7.15",
    "eslint": "^8.35.0",
    "lodash": "^4.17.21",
    "typescript": "~4.9.4"
  }
}
